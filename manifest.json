{
    "manifest_version": 2,
    "name": "Force Dark Mode",
    "version": "0.1",
    "description": "Force dark mode on all websites",
    "developer": {
    "name": "mhu",
    "url": "https://github.com/mhu"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["force-dark-mode.css"]
        }
    ]
}
